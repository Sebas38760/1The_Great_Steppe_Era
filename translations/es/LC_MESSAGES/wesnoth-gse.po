msgstr "Troll Mercenario"

#. [unit_type], id=Kingdom_Troll_Veteran, race=troll
#: 1The_Great_Steppe_Era/units/kingdom/Kingdom_Trolls.cfg:628
msgid "Troll Veteran"
msgstr "Troll Veterano"

#. [unit_type], id=Kingdom_Troll_Guardian, race=troll
#: 1The_Great_Steppe_Era/units/kingdom/Kingdom_Trolls.cfg:682
msgid "Troll Guardian"
msgstr "Troll Guardián"

#. [unit_type], id=Kingdom_Vassal_Eldest_Son
#: 1The_Great_Steppe_Era/units/kingdom/Kingdom_Vassals.cfg:7
msgid "Ogre Eldest Son"
msgstr "Ogro Hijo Mayor"

#. [unit_type], id=Kingdom_Vassal_Loyal_Bishop, race=ogre
#: 1The_Great_Steppe_Era/units/kingdom/Kingdom_Vassals.cfg:32
msgid "Ogre Loyal Bishop"
msgstr "Ogro Obispo Leal"

#. [unit_type], id=Kingdom_Vassal_Mercenary_Ringleader, race=ogre
#: 1The_Great_Steppe_Era/units/kingdom/Kingdom_Vassals.cfg:119
