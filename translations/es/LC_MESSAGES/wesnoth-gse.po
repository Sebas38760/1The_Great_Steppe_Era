#: 1The_Great_Steppe_Era/units/tumen/Minotaur.cfg:35
#: 1The_Great_Steppe_Era/units/tumen/Ruiner.cfg:32
#: 1The_Great_Steppe_Era/units/tumen/Shaman.cfg:28
#: 1The_Great_Steppe_Era/units/tumen/Tempest_Lord.cfg:29
msgid "club"
msgstr "garrote"

#. [attack], type=pierce, range=ranged
#. [attack], type=fire, range=ranged
#. [then]
#: 1The_Great_Steppe_Era/units/kingdom/Kingdom_Crossbow_Halfogres.cfg:53
#: 1The_Great_Steppe_Era/units/kingdom/Kingdom_Crossbow_Halfogres.cfg:63
